<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>ButlerSDK Core Library</Title>
    <Version>1.0.0.0-PublicPreview</Version>
    <IncludeBuildOutput>false</IncludeBuildOutput> <!-- Don't ship an empty DLL for this project -->
	  <!-- 2. Tell NuGet to be silent about the empty lib folder -->
	  <NoWarn>$(NoWarn);NU5128</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\ProjectDependants\JerryChatbox\ButlerSDK.csproj" />
  </ItemGroup>

</Project>
